@model FakturyMVC.Models.InvoicesViewModel


<div class="">
    <table class="table table-striped table-hover margin-top-10" id="resultTable">
        <tr title="title">
            <th>Numer</th>
            <th>Tytuł</th>
            <th>Data</th>
            <th>Sprzedawca</th>
            <th>Kupiec</th>
            <th>Koszt</th>
            <th>Status</th>
        </tr>
        @for (int i = 0; i < Model.InvoiceGeneralsList.Count; i++)
        {
            <tr id=@Model.InvoiceGeneralsList[i].Id name=@i title="@Model.InvoiceGeneralsList[i].Id">
                <th>@Model.InvoiceGeneralsList[i].Number</th>
                <th>@Model.InvoiceGeneralsList[i].Title</th>
                <th>@Model.InvoiceGeneralsList[i].DateOfIssue</th>
                <th>@Model.InvoiceGeneralsList[i].VendorData</th>
                <th>@Model.InvoiceGeneralsList[i].BuyerData</th>                
                <th>@Model.InvoiceGeneralsList[i].OverallCost</th>
                <th>@Model.InvoiceGeneralsList[i].Status</th>
            </tr>
        }

    </table>
</div>


@section Scripts {
    <script src="~/Scripts/jquery-1.10.2.min.js"></script>
    <script src="~/Scripts/custom/bootstrap-datepicker.js"></script>
    @Styles.Render("~/Content/bootstrap-datepicker.css")
    <script src="~/Scripts/custom/HomeScripts.js"></script>

}
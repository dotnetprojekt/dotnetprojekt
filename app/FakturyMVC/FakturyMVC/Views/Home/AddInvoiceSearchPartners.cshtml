@model FakturyMVC.Models.PartnersVievModel

<tr>
    <th>Imię</th>
    <th>Nazwisko</th>
    <th>Firma</th>
    <th>VATIN</th>
</tr> 

@for (int i = 0; i < Model.Partners.Count; i++)
{
    <tr id="vrow @i">
        <th id="vfirstname_@i">@Model.Partners[i].FirstName</th>
        <th id="vlastname_@i">@Model.Partners[i].LastName</th>
        <th id="vcompanyname_@i">@Model.Partners[i].CompanyName</th>
        <th id="vvatin_@i">@Model.Partners[i].Vatin</th>
    </tr>
}

@for (int i = 5; i > Model.Partners.Count; i--)
{
    <tr>
        <th></th><th></th><th></th><th></th>
    </tr>
}

